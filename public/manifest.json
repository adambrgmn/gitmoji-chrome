{
  "manifest_version": 2,

  "name": "Gitmoji",
  "description": "Get gitmojis copied to clipboard",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Copy gimoji"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "permissions": []
}
